  <div class="contain">
    <div class="container-fluid">
      {{>banner}}
    </div>
    <div class="main">
      <div class="leftybar">
        {{>sidebar}}
      </div>
      <div class="righty">
        <div class="maintop">
          {{>maintop}}
        </div>
        {{!-- Contenst Start --}}

        <div class="contents">
          <div class="formtitle">
            <h2> Add New Employee </h2>
          </div>

         <div class="mainform">
           <div class="addemployee">
            <form action="#" method="post" class="" id="addEmployee">
              <table style="width:100%; ">
                <tr>
                  <th style="width:30%;"> </th>
                  <th> </th>
                </tr>
                <tr>
                  <td>
                    <div class="">
                      <label for=""> Tazkira Number: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="Tazkira Number" autofocus required>
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>
                    <div class="">
                      <label for=""> First Name: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="First Name" required>
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>
                    <div class="">
                      <label for=""> Last Name: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="Last Name">
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>
                    <div class="">
                      <label for=""> Father Name: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">                
                        <input type="text" placeholder="Father Name">
                        <label for="">  </label>                     
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>
                    <div class="">
                      <label for=""> Mobile Number: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="Mobile Number" required>
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>
                    <div class="">
                      <label for=""> Email: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="Email">
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>
                <tr>
                  <td>...</td>
                  <td>...</td>
                </tr>
                <tr>
                  <td colspan="2" class="btns">
                    <input type="reset" class="btn btn-secondary" value="Reset" id="resetEmpForm"> 
                    <input type="submit" value="Submit" id="submitEmpForm">
                    <span></span>
                  </td>
                </tr>

              </table>
            </form>
            <span></span>
          </div>
         </div>



        </div>
        {{!-- Contents End --}}
      </div>
    </div>

  </div>





