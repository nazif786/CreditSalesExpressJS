        {{!-- Contenst Start --}}

        <div class="contents">
          <div class="formtitle">
            <h2> Password Reset </h2>
          </div>

         <div class="mainform">
          {{#each alerts}}
                	<div class="alert alert-warning alert-dismissible fade show" role="alert">
                    	{{msg}}
                    	<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                  	</div>  
          {{/each}}
          {{#if msg2}}
                	<div class="alert alert-warning alert-dismissible fade show" role="alert">
                    	{{msg2}}
                    	<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                  	</div>  
          {{/if}}
           <div class="addemployee">
            <form action="passwordReset" method="post" class="" id="addEmployee">
              <table style="width:100%; ">
                <tr>
                  <th style="width:30%;"> </th>
                  <th> </th>
                </tr>
                <tr>
                  <td>
                    <div class="">
                      <label for=""> Tazkira Number: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="Tazkira Number" name="tazkira_id" autofocus >
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>
                    <div class="">
                      <label for=""> Username: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="username" name="username" >
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>

                     <tr>
                  <td>
                    <div class="">
                      <label for=""> Mobile Number: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="text" placeholder="Mobile Number" name="mobile_no" >
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>
                
                  <tr>
                  <td>
                    <div class="">
                      <label for=""> New Password: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">
                      <input type="password" placeholder="password" name="password">
                      <label for="">  </label>
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>
                    <div class="">
                      <label for=""> Confirm new password: </label>
                    </div>
                  </td>
                  <td>
                    <div class="txt_field">                
                        <input type="password" placeholder="confirm password" name="cpassword">
                        <label for="">  </label>                     
                    </div>
                  </td>
                </tr>

                <tr>
                  <td>...</td>
                  <td>...</td>
                </tr>
                <tr>
                  <td colspan="2" class="btns">
                    <input type="reset" value="Reset" id="resetEmpForm"> 
                    <input type="submit" value="Update Password" id="submitEmpForm">
                    <span></span>
                  </td>
                </tr>

              </table>
            </form>
            <span></span>
          </div>


